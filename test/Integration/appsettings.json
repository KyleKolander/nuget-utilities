{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      },
      "FormatterName": "Simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "[yyyy-MM-dd HH::mm:ss] ",
        "ColorBehavior": "Enabled"
      }
    }
  }
}