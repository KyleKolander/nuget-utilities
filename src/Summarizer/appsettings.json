{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.Extensions.Hosting": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.Extensions.Hosting": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.Extensions.Hosting": "Warning"
      },
      "FormatterName": "Simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "[yyyy-MM-dd HH::mm:ss] ",
        "ColorBehavior": "Enabled"
      }
    }
  }
}